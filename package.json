{
  "name": "split-diff",
  "main": "./lib/split-diff",
  "version": "1.3.1",
  "description": "A split pane diff tool.",
  "keywords": [
    "diff",
    "git",
    "split",
    "compare",
    "panes",
    "files",
    "lines",
    "code",
    "difference",
    "visual"
  ],
  "activationCommands": {
    "atom-workspace": [
      "split-diff:toggle",
      "split-diff:enable",
      "split-diff:next-diff",
      "split-diff:prev-diff"
    ]
  },
  "providedServices": {
    "split-diff": {
      "versions": {
        "1.0.0": "provideSplitDiff"
      }
    }
  },
  "repository": "https://github.com/mupchrch/split-diff",
  "license": "MIT",
  "engines": {
    "atom": ">= 1.17 < 2.0.0"
  },
  "dependencies": {
    "diff": "latest"
  }
}
